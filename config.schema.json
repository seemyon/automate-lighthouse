{
  "id": "/Config",
  "type": "object",
  "properties": {
    "source": {
      "type": "array",
      "minItems": 1,
      "uniqueItems": true,
      "items": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string"
          },
          "url": {
            "type": "string"
          }
        },
        "required": ["id", "url"]
      }
    },
    "projectId": {
      "type": "string"
    },
    "datasetId": {
      "type": "string"
    },
    "pubsubTopicId": {
      "type": "string"
    },
    "minTimeBetweenTriggers": {
      "type": "number"
    },
    "lighthouseFlags": {
      "type": "object",
      "properties": {
        "output": {
          "type": "array",
          "items": {
            "type": "string",
            "enum": ["html", "csv", "json"]
          }
        }
      }
    },
    "gcs": {
      "type": "object",
      "properties": {
        "bucketName": {
          "type": "string"
        }
      },
      "required": ["bucketName"]
    }
  },
  "required": ["source", "projectId", "datasetId", "pubsubTopicId", "minTimeBetweenTriggers", "gcs"]
}
